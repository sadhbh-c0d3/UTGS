<xml encoding="utf-8">
    <system>
        <screen id="screen"
            title="Unit Test: Png"
            width="320"
            height="240"
            windowed="1"
            sys_cursor="1"
            />

        <soundcard id="soundcard"
            screen="screen"
            />

        <gui id="gui"
            screen="screen"
            />
    </system>

    <filesystems>
        <dir path="../../themes" point="themes" priority="1000"/>
    </filesystems>

    <include src="themes/graphite/styles.xml"/>

    <widgets>
        <parent id="gui.desktop">
            <image img="img.png" autofill="1"/>
        </parent>
    </widgets>

</xml>
